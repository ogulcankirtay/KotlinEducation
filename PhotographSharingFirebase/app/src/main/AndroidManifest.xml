<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ogulcank.photographsharingfirebase">
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PhotographSharingFirebase">
        <activity
            android:name=".view.PotographSharingActivity"
            android:exported="false" />
        <activity
            android:name=".view.FeddActivity"
            android:exported="false" />
        <activity
            android:name=".view.UserActivty"
            android:exported="true">

            <!--
                Inside this activity you should be having below code for
                intent filter which will check the default activity
            -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>